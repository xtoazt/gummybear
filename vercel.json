{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/chat/socket.io/(.*)",
      "dest": "/src/server.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "GITHUB_TOKEN": "@github_token",
    "GITHUB_OWNER": "@github_owner",
    "GITHUB_REPO": "@github_repo",
    "GITHUB_BRANCH": "@github_branch",
    "JWT_SECRET": "@jwt_secret"
  }
}
