{
  "version": 2,
  "buildCommand": "npm run build:server",
  "functions": {
    "dist/server.bundle.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/server.bundle.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server.bundle.js"
    }
  ]
}