{
  "version": 2,
  "buildCommand": "tsc --project tsconfig.server.json",
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/chat/socket.io/(.*)",
      "dest": "/dist/server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server.js"
    }
  ]
}
