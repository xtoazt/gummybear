{
  "version": 2,
  "buildCommand": "npm run build:server",
  "builds": [
    {
      "src": "dist/server.bundle.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/server.bundle.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server.bundle.js"
    }
  ]
}